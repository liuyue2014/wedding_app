<h1>登录</h1>
<h2>请登录查看更多信息！</h2>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f|%>
      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "登录", class:"btn btn-large btn-primary"%>
    <% end %>
  </div>
</div>
